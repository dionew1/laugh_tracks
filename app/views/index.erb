<h1> Comedians </h1>
<h3> Average Comedian Age: <%= @comedians.average(:age).floor %> </h3>
<ul>
<% @comedians.each do |comedian| %>
  <p>Name: <%= comedian.name %></p>
  <li>Age: <%= comedian.age %> </li>
  <% special = comedian.specials.where(comedian_id: comedian.id) %>
    <% special.each do |s| %>
      <li>Special: <%= s.name %></li>
      <% end %>
<%end%>
